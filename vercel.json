{
  "version": 2,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/menu",
      "destination": "/menu.html"
    },
    {
      "source": "/ar-marker",
      "destination": "/index-ar-marker.html"
    },
    {
      "source": "/mobile",
      "destination": "/index-mobile-simple.html"
    },
    {
      "source": "/desktop",
      "destination": "/index-simple.html"
    },
    {
      "source": "/ar",
      "destination": "/index-ar-simple.html"
    },
    {
      "source": "/no-ar",
      "destination": "/index-no-ar.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
