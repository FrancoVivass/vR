{
  "version": 2,
  "builds": [
    {
      "src": "menu.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/menu.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    },
    {
      "src": "/mobile",
      "dest": "/index-mobile-simple.html"
    },
    {
      "src": "/desktop",
      "dest": "/index-simple.html"
    },
    {
      "src": "/ar",
      "dest": "/index-ar-simple.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
